<%= form_with(model: entrant) do |form| %>
  <% if entrant.errors.any? %>
    <div style="color: red">
      <h2><%= pluralize(entrant.errors.count, "error") %> prohibited this entrant from being saved:</h2>

      <ul>
        <% entrant.errors.each do |error| %>
          <li><%= error.full_message %></li>
        <% end %>
      </ul>
    </div>
  <% end %>

  <div>
    <%= form.label :tournament_id, style: "display: block" %>
