    <%= form.label :division, style: "display: block" %>
    <%= form.text_field :division %>
  </div>

  <div>
    <%= form.label :round, style: "display: block" %>
    <%= form.text_field :round %>
  </div>

  <div>
    <%= form.label :entrant_id, style: "display: block" %>
