  </div>

  <div>
    <%= form.label :opponent_id, style: "display: block" %>
    <%= form.text_field :opponent_id %>
  </div>

  <div>
    <%= form.label :score, style: "display: block" %>
    <%= form.number_field :score %>
  </div>

  <div>
    <%= form.label :opponent_score, style: "display: block" %>
    <%= form.number_field :opponent_score %>
  </div>

  <div>
    <%= form.label :first, style: "display: block" %>
